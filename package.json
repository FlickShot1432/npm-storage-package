    "file-upload",
    "middleware",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "homepage": "https://github.com/FlickShot1432/npm-storage-package#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlickShot1432/npm-storage-package.git"
  },
  "bugs": {
    "url": "https://github.com/FlickShot1432/npm-storage-package/issues"
  },
